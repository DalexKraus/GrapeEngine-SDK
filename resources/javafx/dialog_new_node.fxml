<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
    </children>
    <center>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="420.0" minWidth="10.0" prefWidth="420.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="205.0" minWidth="0.0" prefWidth="0.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Label fx:id="node_title" text="\$NODE_TITLE">
                            <font>
                                <Font size="26.0" />
                            </font>
                            <opaqueInsets>
                                <Insets />
                            </opaqueInsets>
                            <padding>
                                <Insets left="32.0" top="32.0" />
                            </padding>
                        </Label>
                        <Label fx:id="node_class" prefHeight="17.0" prefWidth="407.0" text="\$NODE_CLASS">
                            <padding>
                                <Insets left="32.0" />
                            </padding>
                        </Label>
                    </children>
                </VBox>
            </children>
        </GridPane>
    </center>
    <left>
        <VBox prefHeight="400.0" prefWidth="180.0" BorderPane.alignment="CENTER">
            <children>
                <Pane fx:id="searchPane" prefHeight="56.0" prefWidth="180.0">
                    <children>
                        <TextField fx:id="node_search" layoutX="16.0" layoutY="16.0" promptText="Search" />
                    </children>
                </Pane>
                <ListView fx:id="node_list" prefHeight="348.0" prefWidth="180.0" />
            </children>
        </VBox>
    </left>
</BorderPane>
